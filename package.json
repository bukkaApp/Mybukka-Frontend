  "description": "Frontend of mybukka app built by the internal engineering team",
  "main": "index.js",
  "private": true,
  "engines": {
    "node": ">=10.0.0",
    "npm": ">=6.0.0"
  },
  "scripts": {
    "start": "npm run build && node server/server.js",
    "start:dev": "cross-env NODE_ENV=development webpack-dev-server --host 0.0.0.0 --hot --mode development",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --mode production",
    "deploy": "npm run build && firebase deploy",
    "babel": "node_modules/.bin/babel src -d functions/src",
    "test": "jest test --u --runInBand",
    "test:watch": "jest test --u --runInBand --watch"
  },
